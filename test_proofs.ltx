\EnableBpAbbreviations

\newcommand{\IMPLE}[1]{\scriptstyle\to\mathrm{E}^{#1}}
\newcommand{\IMPLI}[1]{\scriptstyle\to\mathrm{I}^{#1}}
\newcommand{\CONJE}[1]{\scriptstyle\land\mathrm{E}^{#1}}
\newcommand{\CONJI}[1]{\scriptstyle\land\mathrm{I}^{#1}}
\newcommand{\DISJE}[1]{\scriptstyle\lor\mathrm{E}^{#1}}
\newcommand{\DISJI}[1]{\scriptstyle\lor\mathrm{I}^{#1}}

\newcommand{\MARKER}[1]{\RightLabel{\scriptsize(#1)}}
\newcommand{\LABEL}[1]{\RightLabel{$#1$}}

\newcommand{\DNE}[1]{\lnot\lnot#1\to#1}
\newcommand{\LEM}[1]{#1\lor\lnot#1}
\newcommand{\DGP}[2]{(#1\to#2)\lor(#2\to#1)}
\newcommand{\PP}[2]{((#1\to#2)\to#1)\to#1}
\newcommand{\WPP}[2]{\lnot#1\to\lnot\lnot(#1\to#2)}

