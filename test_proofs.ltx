\usepackage{graphicx}
\usepackage{amssymb}
\usepackage{epstopdf}
\DeclareGraphicsRule{.tif}{png}{.png}{`convert #1 `dirname #1`/`basename #1 .tif`.png}

\EnableBpAbbreviations

\newcommand{\NEGE}{\lnot\mathrm{E}}
\newcommand{\IMPLE}{\to\mathrm{E}}
\newcommand{\IMPLI}[1]{\to\mathrm{I}^{#1}}
\newcommand{\CONJE}{\land\mathrm{E}}
\newcommand{\CONJI}{\land\mathrm{I}}
\newcommand{\DISJE}[1]{\lor\mathrm{E}^{#1}}
\newcommand{\DISJI}{\lor\mathrm{I}}

\newcommand{\ASSUME}[2]{\AxiomC{}\MARKER{#2}\UnaryInfC{#1}}
\newcommand{\MARKER}[1]{\RightLabel{\scriptsize(#1)}}
\newcommand{\LABEL}[1]{\RightLabel{$\scriptstyle#1$}}

\newcommand{\DNE}[1]{\lnot\lnot#1\to#1}
\newcommand{\LEM}[1]{#1\lor\lnot#1}
\newcommand{\DGP}[2]{(#1\to#2)\lor(#2\to#1)}
\newcommand{\PP}[2]{((#1\to#2)\to#1)\to#1}
\newcommand{\WPP}[2]{\lnot#1\to\lnot\lnot(#1\to#2)}
\newcommand{\WLEM}[1]{\LEM{#1}}

\title{Proof Tests}
\author{Miles Rout}
\date{\today}

\begin{document}

\let\temp\phi
\let\phi\varphi
\let\varphi\temp

\let\temp\theta
\let\theta\vartheta
\let\vartheta\temp

\maketitle

\begin{prooftree}
	\AxiomC{}
	\MARKER{1}
	\UnaryInfC{$A$}
	\AxiomC{$B$}
	\BinaryInfC{$B$}
	\LABEL{\IMPLE{1}}
	\UnaryInfC{$A \to B$}
\end{prooftree}

Examples of the commands above:

\begin{prooftree}
	\AXC{}
	\UIC{$\DNE{\phi}$}
	\UIC{$\LEM{\phi}$}
	\UIC{$\DGP{\phi}{\theta}$}
	\UIC{$\PP{\phi}{\theta}$}
	\UIC{$\WPP{\phi}{\theta}$}
\end{prooftree}

$\mathrm{PP}_{\DGP{P}{Q},P} \implies \mathrm{DGP}_{P,Q}$:

\begin{prooftree}
	\ASSUME{$P$}{1}
	\ASSUME{$Q$}{2}
	\BIC{$Q$}
	\LABEL{\IMPLI{1}}
	\UIC{$P \to Q$}
	\LABEL{\DISJI}
	\UIC{$\DGP{P}{Q}$}
	\ASSUME{$(\DGP{P}{Q}) \to P$}{3}
	\LABEL{\IMPLE}
	\BIC{$P$}
	\LABEL{\IMPLI{2}}
	\UIC{$Q \to P$}
	\LABEL{\DISJI}
	\UIC{$P \to Q \lor Q \to P$}
	\LABEL{\IMPLI{3}}
	\UIC{$((\DGP{P}{Q}) \to P)\to\DGP{P}{Q}$}
	\LABEL{PP_{\DGP{P}{Q},P}}
	\UIC{$\DGP{P}{Q}$}
\end{prooftree}

$\mathrm{PP}_{\bot,\psi} \implies \mathrm{WPP}_{\phi,\psi}$:

\begin{prooftree}
	\ASSUME{$\phi\to\bot$}{3}
	\ASSUME{$\bot\to\psi$}{1}
	\BIC{$\phi\to\psi$}
	\ASSUME{$(\phi\to\psi)\to\bot$}{2}
	\BIC{$\bot$}
	\LABEL{\IMPLI{1}}
	\UIC{$(\bot\to\psi)\to\bot$}
	\LABEL{PP_{\bot,\psi}}
	\UIC{$\bot$}
	\LABEL{\IMPLI{2}}
	\UIC{$((\phi\to\psi)\to\bot)\to\bot$}
	\LABEL{\IMPLI{3}}
	\UIC{$\lnot\phi\to\lnot\lnot(\phi\to\psi)$}
\end{prooftree}

$\mathrm{DNE}_{\phi} \implies \mathrm{PP}_{\phi,\psi}$:

\begin{prooftree}
	\ASSUME{$(\phi\to\psi)\to\phi$}{6}
	\ASSUME{$\phi\to\psi$}{1}
	\BIC{$\phi$}
	\ASSUME{$\neg\phi$}{5}
	\LABEL{\NEGE}
	\BIC{$\bot$}
	\LABEL{\IMPLI{1}}
	\UIC{$\neg(\phi\to\psi)$}
	\ASSUME{$\phi$}{3}
	\ASSUME{$\neg\psi$}{2}
	\LABEL{\CONJI}
	\BIC{$\phi\land\neg\psi$}
	\ASSUME{$\neg(\phi\land\neg\psi)$}{4}
	\LABEL{\NEGE}
	\BIC{$\bot$}
	\LABEL{\IMPLI{2}}
	\UIC{$\lnot\lnot\psi$}
	\LABEL{\mathrm{DNE}_\psi}
	\UIC{$\psi$}
	\LABEL{\IMPLI{3}}
	\UIC{$\phi\to\psi$}
	\LABEL{\NEGE}
	\BIC{$\bot$}
	\LABEL{\IMPLI{4}}
	\UIC{$\lnot\lnot(\phi\land\lnot\psi)$}
	\LABEL{\mathrm{DNE}_{\phi\land\lnot\psi}}
	\UIC{$\phi\land\lnot\psi$}
	\LABEL{\CONJE}
	\UIC{$\phi$}
	\LABEL{\NEGE}
	\UIC{$\bot$}
	\LABEL{\IMPLI{5}}
	\UIC{$\lnot\lnot\phi$}
	\LABEL{\mathrm{DNE}_{\phi}}
	\UIC{$\phi$}
	\LABEL{\IMPLI{6}}
	\UIC{$((\phi\to\psi)\to\phi)\to\phi$}
\end{prooftree}

$\mathrm{DNE}_\phi \implies \mathrm{EFQ}_\phi$:

\begin{prooftree}
	\ASSUME{$\lnot\phi$}{1}
	\AXC{$\bot$}
	\LABEL{\CONJI}
	\BIC{$\lnot\phi\land\bot$}
	\LABEL{\CONJE}
	\UIC{$\bot$}
	\LABEL{\IMPLI{1}}
	\UIC{$\lnot\phi\to\bot$}
	\UIC{$\lnot\lnot\phi$}
	\LABEL{\mathrm{DNE}_\phi}
	\UIC{$\phi$}
\end{prooftree}

$\mathrm{PP}_{\LEM{\phi},\bot} \implies \mathrm{LEM}_\phi$:

\begin{prooftree}
	\ASSUME{$(\LEM{\phi})\to\bot$}{2}
	\ASSUME{$\phi$}{1}
	\LABEL{\DISJI}
	\UIC{$\LEM{\phi}$}
	\LABEL{\IMPLE}
	\BIC{$\bot$}
	\LABEL{\IMPLI{1}}
	\UIC{$\lnot\phi$}
	\LABEL{\DISJI}
	\UIC{$\LEM{\phi}$}
	\LABEL{\IMPLI{2}}
	\UIC{$((\LEM{\phi})\to\bot)\to(\LEM{\phi})$}
	\LABEL{\mathrm{PP}_{\LEM{\phi},\bot}}
	\UIC{$\LEM{\phi}$}
\end{prooftree}

$\mathrm{DGP}_{\phi,\lnot\phi} \implies \mathrm{WLEM}_\phi$:

\begin{prooftree}
	\AXC{}
	\LABEL{\mathrm{DGP}_{\phi,\lnot\phi}}
	\UIC{$(\phi\to\lnot\phi)\lor(\lnot\phi\to\phi)$}

	\ASSUME{$\phi$}{1}
	\AXC{$\phi\to\lnot\phi$}
	\LABEL{\IMPLE}
	\BIC{$\lnot\phi$}
	\LABEL{\NEGE}
	\UIC{$\bot$}
	\LABEL{\IMPLI{1}}
	\UIC{$\lnot\phi$}
	\LABEL{\DISJI}
	\UIC{$\WLEM{\phi}$}

	\ASSUME{$\lnot\phi$}{2}
	\AXC{$\lnot\phi\to\phi$}
	\LABEL{\IMPLE}
	\BIC{$\phi$}
	\LABEL{\NEGE}
	\UIC{$\bot$}
	\LABEL{\IMPLI{2}}
	\UIC{$\lnot\lnot\phi$}
	\LABEL{\DISJI}
	\UIC{$\WLEM{\phi}$}

	\TIC{$\WLEM{\phi}$}
\end{prooftree}


\end{document} 










 